{
  "version": 2,
  "name": "elzatona-admin",
  "buildCommand": "cd ../.. && npx nx build admin",
  "outputDirectory": ".next",
  "installCommand": "cd ../.. && npm install",
  "framework": "nextjs",
  "functions": {
    "app/api/**/*.ts": {
      "maxDuration": 10
    }
  },
  "env": {
    "NEXT_PUBLIC_APP_NAME": "GreatFrontendHub Admin"
  },
  "build": {
    "env": {
      "NX_DAEMON": "false",
      "NX_PARALLEL": "1"
    }
  }
}
