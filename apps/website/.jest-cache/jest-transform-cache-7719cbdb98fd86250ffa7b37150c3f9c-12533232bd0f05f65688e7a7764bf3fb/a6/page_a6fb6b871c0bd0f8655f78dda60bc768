        });
    }
    // List View
    const uniqueSections = getUniqueSections();
    const uniqueDifficulties = getUniqueDifficulties();
    const filteredItems = filterSection === 'all' && filterDifficulty === 'all' ? items : items.filter((item)=>{
        const sectionMatch = filterSection === 'all' || item.section === filterSection;
        const difficultyMatch = filterDifficulty === 'all' || item.difficulty === filterDifficulty;
        return sectionMatch && difficultyMatch;
    });
    return /*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
