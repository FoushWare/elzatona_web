"use strict";
Object.defineProperty(exports, "__esModule", {
    value: true
});
require("@testing-library/jest-dom");
const _flashcards = require("../lib/flashcards");
describe('flashcards utils', ()=>{
    beforeEach(()=>{
        window.localStorage.clear();
    });
    it('adds, loads and dedupes by id', ()=>{
        const f = {
            id: 'qX',
            question: 'Explain closures',
            addedAt: Date.now()
        };
        (0, _flashcards.addFlashcard)(f);
        (0, _flashcards.addFlashcard)(f);
        const list = (0, _flashcards.loadFlashcards)();
        expect(list).toHaveLength(1);
        expect((0, _flashcards.isInFlashcards)('qX')).toBe(true);
    });
    it('removes item', ()=>{
        const f = {
            id: 'qY',
            question: 'Explain event loop',
            addedAt: Date.now()
        };
        (0, _flashcards.addFlashcard)(f);
        (0, _flashcards.removeFlashcard)('qY');
        expect((0, _flashcards.isInFlashcards)('qY')).toBe(false);
    });
});

