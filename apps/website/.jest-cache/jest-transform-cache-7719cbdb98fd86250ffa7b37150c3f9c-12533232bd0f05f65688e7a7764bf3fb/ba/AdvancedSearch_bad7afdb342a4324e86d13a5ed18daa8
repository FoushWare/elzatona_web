                                                    children: [
                                                        /*#__PURE__*/ (0, _jsxruntime.jsx)(_label.Label, {
                                                            className: "text-sm font-medium",
                                                            children: "Topic"
                                                        }),
                                                        topic !== 'all' && /*#__PURE__*/ (0, _jsxruntime.jsx)(_button.Button, {
                                                            variant: "ghost",
                                                            size: "sm",
                                                            onClick: ()=>setTopic('all'),
                                                            className: "h-6 px-2 text-xs text-gray-500 hover:text-gray-700",
                                                            children: /*#__PURE__*/ (0, _jsxruntime.jsx)(_lucidereact.X, {
                                                                className: "w-3 h-3"
                                                            })
                                                        })
                                                    ]
                                                }),
                                                /*#__PURE__*/ (0, _jsxruntime.jsxs)(_select.Select, {
                                                    value: topic,
                                                    onValueChange: (value)=>handleFilterChange('topic', value),
                                                    children: [
                                                        /*#__PURE__*/ (0, _jsxruntime.jsx)(_select.SelectTrigger, {
                                                            children: /*#__PURE__*/ (0, _jsxruntime.jsx)(_select.SelectValue, {
                                                                placeholder: "All Topics"
                                                            })
                                                        }),
                                                        /*#__PURE__*/ (0, _jsxruntime.jsxs)(_select.SelectContent, {
                                                            children: [
                                                                /*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
                                                                    className: "p-2",
                                                                    children: /*#__PURE__*/ (0, _jsxruntime.jsx)(_input.Input, {
                                                                        placeholder: "Search topics...",
                                                                        value: topicSearchQuery,
                                                                        onChange: (e)=>setTopicSearchQuery(e.target.value),
                                                                        className: "mb-2"
                                                                    })
                                                                }),
                                                                /*#__PURE__*/ (0, _jsxruntime.jsx)(_select.SelectItem, {
                                                                    value: "all",
                                                                    children: "All Topics"
                                                                }),
                                                                allTopics.filter((topic)=>topic.name?.toLowerCase().includes(topicSearchQuery.toLowerCase())).map((topic)=>/*#__PURE__*/ (0, _jsxruntime.jsx)(_select.SelectItem, {
                                                                        value: topic.id,
                                                                        children: topic.name
                                                                    }, topic.id))
                                                            ]
                                                        })
                                                    ]
                                                })
                                            ]
                                        })
                                    ]
                                })
                            })
                        ]
                    })
                ]
            }),
            totalCount > 0 && /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                className: "flex items-center justify-between",
                children: [
                    /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                        className: "text-sm text-gray-700 dark:text-gray-300",
                        children: [
                            "Showing ",
                            Math.min((currentPage - 1) * pageSize + 1, totalCount),
                            " to",
                            ' ',
                            Math.min(currentPage * pageSize, totalCount),
                            " of ",
                            totalCount,
                            ' ',
                            "questions"
                        ]
                    }),
                    /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                        className: "flex items-center space-x-4",
                        children: [
                            /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                                className: "flex items-center space-x-2",
                                children: [
                                    /*#__PURE__*/ (0, _jsxruntime.jsx)("span", {
                                        className: "text-sm text-gray-600 dark:text-gray-400",
                                        children: "Show:"
                                    }),
                                    /*#__PURE__*/ (0, _jsxruntime.jsxs)(_select.Select, {
                                        value: size.toString(),
                                        onValueChange: (value)=>{
                                            const newSize = parseInt(value);
                                            setSize(newSize);
                                            onPageSizeChange?.(newSize);
                                        },
                                        children: [
                                            /*#__PURE__*/ (0, _jsxruntime.jsx)(_select.SelectTrigger, {
                                                className: "w-20",
                                                children: /*#__PURE__*/ (0, _jsxruntime.jsx)(_select.SelectValue, {})
                                            }),
                                            /*#__PURE__*/ (0, _jsxruntime.jsxs)(_select.SelectContent, {
                                                children: [
                                                    /*#__PURE__*/ (0, _jsxruntime.jsx)(_select.SelectItem, {
                                                        value: "5",
                                                        children: "5"
                                                    }),
                                                    /*#__PURE__*/ (0, _jsxruntime.jsx)(_select.SelectItem, {
                                                        value: "10",
                                                        children: "10"
                                                    }),
                                                    /*#__PURE__*/ (0, _jsxruntime.jsx)(_select.SelectItem, {
                                                        value: "20",
                                                        children: "20"
                                                    }),
                                                    /*#__PURE__*/ (0, _jsxruntime.jsx)(_select.SelectItem, {
                                                        value: "50",
                                                        children: "50"
                                                    }),
                                                    /*#__PURE__*/ (0, _jsxruntime.jsx)(_select.SelectItem, {
                                                        value: "100",
                                                        children: "100"
                                                    })
                                                ]
                                            })
                                        ]
                                    })
                                ]
                            }),
                            /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                                className: "flex items-center space-x-2",
                                children: [
                                    /*#__PURE__*/ (0, _jsxruntime.jsx)(_button.Button, {
                                        variant: "outline",
                                        size: "sm",
                                        onClick: ()=>{
                                            const newPage = page - 1;
                                            setPage(newPage);
                                            onPageChange?.(newPage);
                                        },
                                        disabled: page === 1,
                                        children: /*#__PURE__*/ (0, _jsxruntime.jsx)(_lucidereact.ChevronLeft, {
                                            className: "h-4 w-4"
                                        })
                                    }),
                                    /*#__PURE__*/ (0, _jsxruntime.jsxs)("span", {
                                        className: "text-sm text-gray-600 dark:text-gray-400",
                                        children: [
                                            "Page ",
                                            page,
                                            " of ",
                                            internalTotalPages
                                        ]
                                    }),
                                    /*#__PURE__*/ (0, _jsxruntime.jsx)(_button.Button, {
                                        variant: "outline",
                                        size: "sm",
                                        onClick: ()=>{
                                            const newPage = page + 1;
                                            setPage(newPage);
                                            onPageChange?.(newPage);
                                        },
                                        disabled: page >= internalTotalPages,
                                        children: /*#__PURE__*/ (0, _jsxruntime.jsx)(_lucidereact.ChevronRight, {
                                            className: "h-4 w-4"
                                        })
                                    })
                                ]
                            })
                        ]
                    })
                ]
            }),
            /*#__PURE__*/ (0, _jsxruntime.jsx)(_card.Card, {
                children: /*#__PURE__*/ (0, _jsxruntime.jsx)(_card.CardContent, {
                    className: "p-4",
                    children: /*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
                        className: "flex items-center justify-between",
                        children: /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                            className: "flex items-center space-x-4",
                            children: [
                                /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                                    className: "flex items-center space-x-2",
                                    children: [
                                        /*#__PURE__*/ (0, _jsxruntime.jsx)(_lucidereact.BarChart3, {
                                            className: "w-4 h-4 text-green-500"
                                        }),
                                        /*#__PURE__*/ (0, _jsxruntime.jsxs)("span", {
                                            className: "text-sm font-medium",
                                            children: [
                                                totalCount,
                                                " results found"
                                            ]
                                        })
                                    ]
                                }),
                                searchTime > 0 && /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                                    className: "flex items-center space-x-2",
                                    children: [
                                        /*#__PURE__*/ (0, _jsxruntime.jsx)(_lucidereact.Clock, {
                                            className: "w-4 h-4 text-blue-500"
                                        }),
                                        /*#__PURE__*/ (0, _jsxruntime.jsxs)("span", {
                                            className: "text-sm text-gray-600 dark:text-gray-400",
                                            children: [
                                                searchTime,
                                                "ms"
                                            ]
                                        })
                                    ]
                                })
                            ]
                        })
                    })
                })
            })
        ]
    });
}

