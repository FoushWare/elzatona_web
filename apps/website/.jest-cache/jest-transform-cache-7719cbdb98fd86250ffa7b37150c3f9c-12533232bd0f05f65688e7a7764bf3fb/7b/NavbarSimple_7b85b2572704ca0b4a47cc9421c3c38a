                                                    onClick: ()=>setIsOpen(false),
                                                    children: "Sign In"
                                                })
                                            ]
                                        })
                                    ]
                                })
                            ]
                        })
                    })
                ]
            }),
            isModeSwitching && /*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
                className: "fixed inset-0 z-[100] bg-black/40 backdrop-blur-sm flex items-center justify-center",
                children: /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                    className: "bg-white dark:bg-gray-800 rounded-2xl p-6 shadow-2xl flex flex-col items-center space-y-4",
                    children: [
                        /*#__PURE__*/ (0, _jsxruntime.jsx)(_lucidereact.Loader2, {
                            className: "w-8 h-8 text-indigo-600 dark:text-indigo-400 animate-spin"
                        }),
                        /*#__PURE__*/ (0, _jsxruntime.jsxs)("p", {
                            className: "text-gray-900 dark:text-white font-medium",
                            children: [
                                "Switching to",
                                ' ',
                                switchingToMode === 'guided' ? 'Guided' : 'Free Style',
                                ' ',
                                "Learning..."
                            ]
                        })
                    ]
                })
            })
        ]
    });
};
