// v1.0 - simple localStorage-backed flashcards helper
"use strict";
Object.defineProperty(exports, "__esModule", {
    value: true
});
function _export(target, all) {
    for(var name in all)Object.defineProperty(target, name, {
        enumerable: true,
        get: Object.getOwnPropertyDescriptor(all, name).get
    });
}
_export(exports, {
    get addFlashcard () {
        return addFlashcard;
    },
    get isInFlashcards () {
        return isInFlashcards;
    },
    get loadFlashcards () {
        return loadFlashcards;
    },
    get removeFlashcard () {
        return removeFlashcard;
    },
    get saveFlashcards () {
        return saveFlashcards;
    }
});
const STORAGE_KEY = 'flashcards:v1';
function loadFlashcards() {
    if (typeof window === 'undefined') return [];
    try {
        const raw = localStorage.getItem(STORAGE_KEY);
        return raw ? JSON.parse(raw) : [];
    } catch (_) {
        return [];
    }
}
function saveFlashcards(items) {
    if (typeof window === 'undefined') return;
    try {
        localStorage.setItem(STORAGE_KEY, JSON.stringify(items));
    } catch (_) {
    // no-op
    }
}
function addFlashcard(item) {
    const items = loadFlashcards();
    if (items.some((i)=>i.id === item.id)) return; // dedupe by question id
    items.unshift(item);
    saveFlashcards(items);
}
function removeFlashcard(questionId) {
    const items = loadFlashcards().filter((i)=>i.id !== questionId);
    saveFlashcards(items);
}
function isInFlashcards(questionId) {
    return loadFlashcards().some((i)=>i.id === questionId);
}

