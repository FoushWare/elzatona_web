const _server = require("next/server");
const _supabasejs = require("@supabase/supabase-js");
const supabaseUrl = process.env.NEXT_PUBLIC_SUPABASE_URL;
const supabaseServiceRoleKey = process.env.SUPABASE_SERVICE_ROLE_KEY;
const supabase = (0, _supabasejs.createClient)(supabaseUrl, supabaseServiceRoleKey);
async function GET(request, { params }) {
    try {
        console.log('üîÑ API: Fetching frontend task by ID...');
        const { id } = await params;
        if (!id) {
            return _server.NextResponse.json({
                success: false,
                error: 'Task ID is required'
            }, {
                status: 400
            });
        }
        // Fetch task from Supabase
        const { data: taskDoc, error } = await supabase.from('frontend_tasks').select('*').eq('id', id).single();
        if (error || !taskDoc) {
            return _server.NextResponse.json({
                success: false,
                error: 'Task not found'
            }, {
                status: 404
            });
        }
        const taskData = {
            id: taskDoc.id,
            ...taskDoc
        };
        console.log(`‚úÖ API: Frontend task fetched: ${taskData.title}`);
        const response = {
            success: true,
            data: taskData
        };
        return _server.NextResponse.json(response);
    } catch (error) {
        console.error('‚ùå API: Error fetching frontend task:', error);
        return _server.NextResponse.json({
            success: false,
            error: 'Failed to fetch frontend task'
        }, {
            status: 500
        });
    }
}
async function PUT(request, { params }) {
    try {
        console.log('üîÑ API: Updating frontend task...');
        const { id } = await params;
        const body = await request.json();
        if (!id) {
            return _server.NextResponse.json({
                success: false,
                error: 'Task ID is required'
            }, {
                status: 400
            });
        }
        // Validate required fields
        if (!body.title || !body.description || !body.category) {
            return _server.NextResponse.json({
                success: false,
                error: 'Missing required fields'
            }, {
                status: 400
            });
        }
        // Check if task exists
        const { data: taskDoc, error: fetchError } = await supabase.from('frontend_tasks').select('*').eq('id', id).single();
        if (fetchError || !taskDoc) {
            return _server.NextResponse.json({
                success: false,
                error: 'Task not found'
            }, {
                status: 404
            });
        }
        // Update task in Supabase
        const { error: updateError } = await supabase.from('frontend_tasks').update({
            ...body,
            updated_at: new Date().toISOString()
        }).eq('id', id);
        console.log(`‚úÖ API: Frontend task updated: ${id}`);
        const response = {
            success: true,
            data: {
                id
            }
        };
        return _server.NextResponse.json(response);
    } catch (error) {
        console.error('‚ùå API: Error updating frontend task:', error);
        return _server.NextResponse.json({
            success: false,
            error: 'Failed to update frontend task'
        }, {
            status: 500
        });
    }
}
async function DELETE(request, { params }) {
    try {
        console.log('üîÑ API: Deleting frontend task...');
        const { id } = await params;
        if (!id) {
            return _server.NextResponse.json({
                success: false,
                error: 'Task ID is required'
            }, {
                status: 400
            });
        }
        // Check if task exists
        const { data: taskDoc, error: fetchError } = await supabase.from('frontend_tasks').select('*').eq('id', id).single();
        if (fetchError || !taskDoc) {
            return _server.NextResponse.json({
                success: false,
                error: 'Task not found'
            }, {
                status: 404
            });
        }
        // Delete task from Supabase
        const { error: deleteError } = await supabase.from('frontend_tasks').delete().eq('id', id);
        if (deleteError) {
            throw deleteError;
        }
        console.log(`‚úÖ API: Frontend task deleted: ${id}`);
        const response = {
            success: true,
            data: {
                id
            }
        };
        return _server.NextResponse.json(response);
    } catch (error) {
        console.error('‚ùå API: Error deleting frontend task:', error);
        return _server.NextResponse.json({
            success: false,
            error: 'Failed to delete frontend task'
        }, {
            status: 500
        });
    }
