{
  "14996743613025305413": {
    "build": {
      "command": "next build",
      "options": {
        "cwd": "apps/admin",
        "tty": false
      },
      "dependsOn": ["^build"],
      "cache": true,
      "inputs": [
        "default",
        "^production",
        {
          "externalDependencies": ["next"]
        }
      ],
      "outputs": [
        "{workspaceRoot}/apps/admin/.next/!(cache)/**/*",
        "{workspaceRoot}/apps/admin/.next/!(cache)"
      ]
    },
    "dev": {
      "continuous": true,
      "command": "next dev",
      "options": {
        "cwd": "apps/admin"
      }
    },
    "start": {
      "continuous": true,
      "command": "next start",
      "options": {
        "cwd": "apps/admin"
      },
      "dependsOn": ["build"]
    },
    "serve-static": {
      "continuous": true,
      "command": "next start",
      "options": {
        "cwd": "apps/admin"
      },
      "dependsOn": ["build"]
    },
    "build-deps": {
      "dependsOn": ["^build"]
    },
    "watch-deps": {
      "continuous": true,
      "dependsOn": ["build-deps"],
      "command": "npx nx watch --projects admin --includeDependentProjects -- npx nx build-deps admin"
    }
  },
