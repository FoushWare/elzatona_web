{
  "name": "zatona-web",
  "version": "1.0.0",
  "description": "A platform for frontend developers to practice coding challenges and prepare for technical interviews",
  "private": true,
  "scripts": {
    "dev": "nx serve elzatona-web",
    "dev:admin": "nx serve admin",
    "dev:turbo": "nx serve elzatona-web --turbopack",
    "build": "nx build elzatona-web",
    "build:admin": "nx build admin",
    "build:all": "nx run-many --target=build --all",
    "build:check": "nx build elzatona-web --turbopack && echo '✅ Build successful' || (echo '❌ Build failed' && exit 1)",
    "start": "nx start elzatona-web",
    "start:admin": "nx start admin",
    "test": "nx run-many --target=test --all",
    "test:ui": "nx test ui",
    "test:auth": "nx test auth",
    "test:database": "nx test database",
    "test:utils": "nx test utils",
    "lint:all": "nx run-many --target=lint --all",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write .",
    "prepare": "husky",
    "pre-commit:switch": "bash scripts/switch-pre-commit.sh",
    "pre-commit:fast": "cp .husky/pre-commit-fast .husky/pre-commit && echo '✅ Switched to FAST pre-commit checks'",
    "pre-commit:standard": "cp .husky/pre-commit .husky/pre-commit && echo '✅ Switched to STANDARD pre-commit checks'",
    "pre-commit:full": "cp .husky/pre-commit-full .husky/pre-commit && echo '✅ Switched to FULL pre-commit checks'",
    "pre-commit:e2e": "cp .husky/pre-commit-e2e .husky/pre-commit && echo '✅ Switched to E2E pre-commit checks with auto-resolution'",
    "cleanup": "bash scripts/cleanup-build.sh",
    "cleanup:deep": "bash scripts/cleanup-build.sh --deep",
    "cleanup:project": "node scripts/cleanup-project.js",
    "health-check": "node scripts/project-health-check.js",
    "maintenance": "npm run cleanup:project && npm run health-check",
    "setup-env": "node scripts/setup-env.mjs",
    "test:admin-login": "node scripts/run-admin-login-tests.mjs",
    "test:admin-login:fast": "npx jest tests/admin/admin-login-api.test.ts --verbose",
    "test:admin-login:watch": "npx jest tests/admin/ --watch",
    "test:admin-login:coverage": "npx jest tests/admin/ --coverage",
    "test:admin": "node scripts/run-admin-tests.mjs",
    "test:admin:all": "node scripts/run-admin-tests.mjs all",
    "test:admin:watch": "node scripts/run-admin-tests.mjs watch",
    "test:admin:auth": "node scripts/run-admin-tests.mjs category \"Authentication Tests\"",
    "test:admin:api": "node scripts/run-admin-tests.mjs category \"API Tests\"",
    "test:admin:ui": "node scripts/run-admin-tests.mjs category \"UI Component Tests\"",
    "test:admin:integration": "node scripts/run-admin-tests.mjs category \"Integration Tests\"",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:e2e:install": "playwright install",
    "test:e2e:codegen": "playwright codegen",
    "test:e2e:chromium": "playwright test --project=chromium",
    "test:e2e:firefox": "playwright test --project=firefox",
    "test:e2e:webkit": "playwright test --project=webkit",
    "test:e2e:mobile": "playwright test --project='Mobile Chrome' --project='Mobile Safari'",
    "git-hooks:standard": "bash scripts/configure-git-hooks.sh standard",
    "git-hooks:admin-tests": "bash scripts/configure-git-hooks.sh admin-tests",
    "git-hooks:fast": "bash scripts/configure-git-hooks.sh fast",
    "git-hooks:comprehensive": "bash scripts/configure-git-hooks.sh comprehensive",
    "git-hooks:status": "bash scripts/configure-git-hooks.sh status"
  },
  "dependencies": {
    "@daily-co/daily-js": "^0.84.0",
    "@heroicons/react": "^2.2.0",
    "@monaco-editor/react": "^4.7.0",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@reactour/tour": "^3.8.0",
    "@tanstack/react-query": "^5.90.3",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/swagger-jsdoc": "^6.0.4",
    "axios": "^1.11.0",
    "bcryptjs": "^3.0.2",
    "cheerio": "^1.1.2",
    "class-variance-authority": "^0.7.1",
    "cloudinary": "^2.7.0",
    "clsx": "^2.1.1",
    "critters": "^0.0.23",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "firebase": "^10.13.0",
    "jotai": "^2.15.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.542.0",
    "next": "15.5.1",
    "next-auth": "^4.24.11",
    "node-cron": "^4.2.1",
    "node-fetch": "^2.7.0",
    "puppeteer": "^24.17.1",
    "react": "^18.3.1",
    "react-day-picker": "^9.11.0",
    "react-dom": "^18.3.1",
    "react-resizable-panels": "^2.0.0",
    "sonner": "^2.0.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-react": "^5.29.4",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@nx/eslint": "^21.6.5",
    "@nx/jest": "^21.6.5",
    "@nx/next": "^21.6.5",
    "@nx/vite": "^21.6.5",
    "@playwright/test": "^1.56.0",
    "@supabase/supabase-js": "^2.75.1",
    "@swc-node/register": "^1.11.1",
    "@swc/core": "^1.13.5",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "autoprefixer": "^10.4.21",
    "eslint": "^9",
    "eslint-config-next": "15.5.1",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "lint-staged": "^16.1.5",
    "nx": "^21.6.5",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.17",
    "ts-jest": "^29.4.5",
    "tsx": "^4.20.5",
    "typescript": "^5"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "nx": {}
}
